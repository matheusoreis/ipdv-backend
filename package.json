{
  "name": "ipdv-backend",
  "version": "1.0.0",
  "scripts": {
    "start": "tsx index.ts",
    "dev": "tsx watch index.ts",
    "format": "prettier --write .",
    "db:up": "tsx --no-warnings ./node_modules/knex/bin/cli.js --knexfile knexfile.ts --env production migrate:latest",
    "db:make": "tsx --no-warnings ./node_modules/knex/bin/cli.js --knexfile knexfile.ts --env production migrate:make",
    "db:seed:make": "tsx --no-warnings ./node_modules/knex/bin/cli.js --knexfile knexfile.ts --env production seed:make",
    "db:seed:up": "tsx --no-warnings ./node_modules/knex/bin/cli.js --knexfile knexfile.ts --env production seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusoreis/ipdv-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matheusoreis/ipdv-backend/issues"
  },
  "homepage": "https://github.com/matheusoreis/ipdv-backend#readme",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
